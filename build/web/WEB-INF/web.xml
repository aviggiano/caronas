<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>AddRoute</servlet-name>
        <servlet-class>servlets.AddRoute</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetRoute</servlet-name>
        <servlet-class>servlets.GetRoute</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteRoute</servlet-name>
        <servlet-class>servlets.DeleteRoute</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchRide</servlet-name>
        <servlet-class>servlets.SearchRide</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddRoute</servlet-name>
        <url-pattern>/AddRoute</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetRoute</servlet-name>
        <url-pattern>/GetRoute</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteRoute</servlet-name>
        <url-pattern>/DeleteRoute</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchRide</servlet-name>
        <url-pattern>/SearchRide</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
